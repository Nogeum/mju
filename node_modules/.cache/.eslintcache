) => {\n    e.stopPropagation();\n    changeLike(likeItem.restid);\n  };\n\n  useEffect(() => {\n    getRestData();\n    calcRate();\n  }, []);\n\n  const img = `https://bucket123478.s3.ap-northeast-2.amazonaws.com/RestaurantPhoto/${likeItem.restid}_1.jpg`;\n  return (\n    <>\n      {restData && (\n        <div onClick={moveToDetail} className={styles.item}>\n          <button className={styles.zzim} onClick={changeZzim}>\n            <FontAwesomeIcon icon={faHeartFull} className={styles.fullHeart} />\n          </button>\n          <img src={img} alt='list_image' />\n          <div className={styles.data_container}>\n            <p className={styles.title}>{restData.info.NAME}</p>\n            <p>{restData.info.TIME}</p>\n            <p>{restData.info.NUMBER}</p>\n            <ReactStars count={5} value={rate} edit={false} size={16} />\n          </div>\n        </div>\n      )}\n    </>\n  );\n};\n",{"ruleId":"185","severity":1,"message":"186","line":27,"column":9,"nodeType":"187","messageId":"188","endLine":27,"endColumn":24},{"ruleId":"189","severity":1,"message":"190","line":71,"column":6,"nodeType":"191","endLine":71,"endColumn":16,"suggestions":"192"},{"ruleId":"185","severity":1,"message":"193","line":1,"column":10,"nodeType":"187","messageId":"188","endLine":1,"endColumn":21},{"ruleId":"189","severity":1,"message":"194","line":62,"column":6,"nodeType":"191","endLine":62,"endColumn":22,"suggestions":"195"},{"ruleId":"185","severity":1,"message":"196","line":4,"column":10,"nodeType":"187","messageId":"188","endLine":4,"endColumn":19},{"ruleId":"185","severity":1,"message":"197","line":8,"column":8,"nodeType":"187","messageId":"188","endLine":8,"endColumn":14},{"ruleId":"185","severity":1,"message":"198","line":2,"column":8,"nodeType":"187","messageId":"188","endLine":2,"endColumn":17},{"ruleId":"185","severity":1,"message":"199","line":15,"column":10,"nodeType":"187","messageId":"188","endLine":15,"endColumn":17},{"ruleId":"185","severity":1,"message":"200",